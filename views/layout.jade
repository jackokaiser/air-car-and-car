          ul.nav.navbar-nav
            li.active
              a(href='cars') cars
            li
              a(href='map') map
            li
              a(href='newcar') new car

    .container-fluid
      .row-fluid
        block body

    script(src='js/lib/jquery-1.11.0.min.js')
